{
    "version": 2,
    "builds": [
      {
        "src": "adricristina/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "adricristina/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/adricristina/index.html"
      }
    ]
  }
  